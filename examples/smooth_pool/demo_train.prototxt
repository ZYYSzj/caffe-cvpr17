layer {
  name: "pool1"
  type: "SmoothPooling"
  bottom: "data"
  top: "pool1"
  param {
    lr_mult: 1
    decay_mult: 0
  }
  smooth_pooling_param {
    has_smooth_blobs: true
    unique_smooth: true
    smooth_filler {
      type: "constant"
      value: 1000
    }
  }
}

layer {
  name: "prob"
  type: "Softmax"
  bottom: "pool1"
  top: "prob"
}

